@model Planit.Core.Node<Planit.Models.Project>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.data.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.dfs(Model)) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.data.Description)
        </td>
    </tr>
}

</table>
